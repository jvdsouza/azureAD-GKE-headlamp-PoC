{
    "compilerOptions": {
      "lib": ["DOM", "ES2022"],
      "declaration": true,
      "declarationMap": true,
      "outDir": "dist-types",
      "esModuleInterop": true,
      "jsx": "react",
      "target": "ES2022",
      "moduleResolution": "node",
      "module": "ES2022",
      "skipLibCheck": true,
      "strict": true,
      "allowSyntheticDefaultImports": true,
      "forceConsistentCasingInFileNames": true
    }
  }
  